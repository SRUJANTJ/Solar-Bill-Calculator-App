{
  "expo": {
    "name": "Solar Calculator",
    "slug": "solar-Calculator",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/solarenergy.jpeg",
    "scheme": "Solar Calculator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/solarenergy.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.stjj.solarCalculator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/solarenergy.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/solarenergy.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
